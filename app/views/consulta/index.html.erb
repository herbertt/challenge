<p id="notice"><%= notice %></p>

<h1>Consulta</h1>

<table>
  <thead>
    <tr>
      <th>User name</th>
      <th>Email</th>
      <th>Empresa</th>
      <th>Faturamento</th>
      <th>Val titulo</th>
      <th>Vencimento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consulta.each do |consultum| %>
      <tr>
        <td><%= consultum.user_name %></td>
        <td><%= consultum.email %></td>
        <td><%= consultum.empresa %></td>
        <td><%= consultum.faturamento %></td>
        <td><%= consultum.val_titulo %></td>
        <td><%= consultum.vencimento %></td>
        <td><%= link_to 'Show', consultum %></td>
        <td><%= link_to 'Edit', edit_consultum_path(consultum) %></td>
        <td><%= link_to 'Destroy', consultum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nova Consulta', new_consultum_path %>
